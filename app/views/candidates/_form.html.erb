<%= f.error_notification %>
<%= f.input :name, input_html: { value: current_user.set_name } %>
<%= f.label :gender %>
<%= f.collection_radio_buttons :gender, human_attribute_genders,
                               :first,
                               :second,
                               checked: human_attribute_genders.first %>
<%= f.input :email, placeholder: t('simple_form.placeholders.candidate.email')%>
<%= f.input :address, input_html: {rows: 3} %>
<%= f.input :contact%>
<%= f.input :dob,
            as: :date,
            html5: true,
            :order => [ :day, :month, :year] %>
